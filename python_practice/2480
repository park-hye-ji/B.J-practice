a,b,c=map(int,input().split())

    

if (a==b and b==c):
    s3= 10000 + a*1000
    print(s3)

if((a==b and a!=c)or(a==c and a!=b)or(c==b and b!=a)):

    if(a==b):
        s=a

    if(b==c):
        s=b

    if(a==c):
        s=c

    s2= 1000 + s *100

    print(s2)

if(a!=b and b!=c and a!=c):

    if(a>b):
        m=a

    else:
        m=b

    if(m<c):
        m=c

    s1= m *100

    print(s1)
